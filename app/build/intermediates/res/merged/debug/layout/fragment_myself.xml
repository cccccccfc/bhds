          android:layout_height="@dimen/dm135"
          android:layout_marginLeft="@dimen/dm016"
          android:layout_marginRight="@dimen/dm016"
          android:weightSum="4"
          android:baselineAligned="false"
          >
        <LinearLayout
            android:id="@+id/ll_myself_property_redpacket"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm010"
              android:src="@mipmap/money"
              />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="　红包　"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_myself_property_recharge"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm010"
              android:src="@mipmap/recharge"
              />
