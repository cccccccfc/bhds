              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="在线充值"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_myself_property_destoonfinancecharge"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm008"
              android:src="@mipmap/eka"
              />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="E卡充值"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_myself_property_integral"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm010"
              android:src="@mipmap/integral"
              />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="我的积分"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>
      </LinearLayout>
      <TextView
          android:layout_width="match_parent"
          android:layout_height="@dimen/dm016"
          android:background="#f4f4f4"
          />
      <RelativeLayout
          android:layout_width="match_parent"
          android:layout_height="@dimen/dm081"
          android:gravity="center_vertical"
          >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="@dimen/dm040"
            android:text="我的服务"
            android:textSize="@dimen/dm026"
            />

      </RelativeLayout>
      <TextView
          android:layout_width="match_parent"
          android:layout_height="@dimen/dm001"
          android:background="#EEEEEE"
          />
      <LinearLayout
          android:layout_width="match_parent"
          android:layout_height="@dimen/dm136"
          android:layout_marginLeft="@dimen/dm016"
          android:layout_marginRight="@dimen/dm016"
          android:weightSum="4"
          android:baselineAligned="false"
          >
        <LinearLayout
            android:id="@+id/ll_myself_service_invitefriends"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm010"
              android:src="@mipmap/inviting"
              />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="邀请好友"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_myself_service_evaluationlist"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm010"
              android:src="@mipmap/evaluate"
              />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="晒单/评价"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_myself_service_enshrine"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm010"
              android:src="@mipmap/collection"
              />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="　收藏　"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_myself_service_footprint"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm010"
              android:src="@mipmap/track"
              />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="　足迹　"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>
      </LinearLayout>
      <LinearLayout
          android:layout_width="match_parent"
          android:layout_height="@dimen/dm135"
          android:layout_marginLeft="@dimen/dm016"
          android:layout_marginRight="@dimen/dm016"
          android:weightSum="4"
          android:baselineAligned="false"
          >
        <LinearLayout
            android:id="@+id/ll_myself_service_location"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm010"
              android:src="@mipmap/add"
              />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="　地址　"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_myself_service_customerservice"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm010"
              android:src="@mipmap/service"
              />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="　客服　"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_myself_service_help"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
          <ImageView
              android:layout_width="@dimen/dm060"
              android:layout_height="@dimen/dm060"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm016"
              android:padding="@dimen/dm010"
              android:src="@mipmap/help"
              />
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginTop="@dimen/dm010"
              android:text="　帮助　"
              android:textSize="@dimen/dm024"
              />
        </LinearLayout>
        <LinearLayout
