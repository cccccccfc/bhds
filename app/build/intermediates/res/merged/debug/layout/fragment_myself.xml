          android:layout_height="@dimen/dm081"
          android:gravity="center"
          >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dm040"
            android:text="我的资产"
            android:textSize="@dimen/dm026"
            />
        <TextView
            android:id="@+id/tv_myself_property_totalassets"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dm030"
            android:drawablePadding="@dimen/dm020"
            android:drawableRight="@mipmap/more"
            android:text="全部资产/充值"
            android:textSize="@dimen/dm024"
            />

      </RelativeLayout>
      <TextView
