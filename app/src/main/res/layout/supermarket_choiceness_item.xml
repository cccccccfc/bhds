<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
  <LinearLayout
      android:id="@+id/lin_supermarket_choiceness"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginTop="@dimen/dm010"
      android:layout_marginLeft="@dimen/dm008"
      android:layout_marginRight="@dimen/dm008"
      android:layout_marginBottom="@dimen/dm020"
      android:orientation="vertical"
      >
    <ImageView
        android:id="@+id/img_supermarket_choiceness"
        android:layout_width="wrap_content"
        android:scaleType="fitXY"
        android:layout_height="@dimen/dm220"
        android:background="@mipmap/supermarket_newproduct"
        />
    <TextView
        android:id="@+id/txe_supermarket_choiceness1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dm015"
        android:layout_marginLeft="@dimen/dm005"
        android:textSize="@dimen/dm026"
        android:text="测试测试测试测试"
        android:maxEms="8"
        android:singleLine="true"
        android:ellipsize="end"
        android:textColor="@color/black"
        />
    <TextView
        android:id="@+id/txe_supermarket_choiceness2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dm005"
        android:layout_marginTop="@dimen/dm010"
        android:textSize="@dimen/dm028"
        android:maxEms="5"
        android:singleLine="true"
        android:ellipsize="end"
        android:text="测试测试"
        android:textColor="@color/price"
        />
  </LinearLayout>

</LinearLayout>
